    <section name="PalMonConfig" type="PalMon.Config.PalMonConfig, PalMonLib, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="log4net-config-file" value="Config\Log.config" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
